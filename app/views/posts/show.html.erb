name: <%= @post.name %>

content_public: <%= @post.content_public %>

<% if @post.user_id == current_user.id %>
    content_private: <%= @post.content_private %>
<% end %>

<% if @post.user_id == current_user.id %>
    <%= link_to "投稿の編集", edit_post_path(@post) %>
    <%= link_to "投稿の削除", post_path(@post), method: :delete , data: {confirm: "削除しますか？" } %>
<% end %>