<h3>投稿の編集</h3>
<%= form_with(model: @post, class: "form-horizontal") do |f| %>
  <div id="errors"></div>
  Title:
  <%= @post.name %>
  <h3><%= f.hidden_field :name, value: @post.name%></h3>

  <div class="form-group">
    <%= f.label :content_private %>
    <%= f.text_field :content_private, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :content_public %>
    <%= f.text_field :content_private, class: "form-control" %>
  </div>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
